   <I18N_LINES>
    <item>
     <LANGUAGE>D</LANGUAGE>
     <LINES>
      <TLINE>
       <TDFORMAT>U1</TDFORMAT>
       <TDLINE>&amp;FUNCTIONALITY&amp;</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>AS</TDFORMAT>
       <TDLINE>Die Klasse CL_ISH_AS_MASTER (Master) dient zur Verwaltung von</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>&lt;DS:CL.CL_ISH_ASM_PARAM&gt;Applikationseinstellungen&lt;/&gt;.</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>AS</TDFORMAT>
       <TDLINE>Durch Aufruf der Methode</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>&lt;DS:CO.CL_ISH_AS_MASTER              CREATE&gt;CREATE&lt;/&gt; kann ein Master</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>instanziert werden.</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>AS</TDFORMAT>
       <TDLINE>Die Methode &lt;DS:CO.CL_ISH_AS_MASTER              GET_PARAM&gt;GET_PARAM&lt;/&gt;</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>dient dazu, eine &lt;DS:CL.CL_ISH_ASM_PARAM&gt;Applikationseinstellung&lt;/&gt; zur</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>weiteren Verarbeitung zu ermitteln. Jede Applikationseinstellung verfügt</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>über Methoden, um deren Werte auszulesen oder zu verändern.</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>/</TDFORMAT>
       <TDLINE>Über die Methode</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>&lt;DS:CO.CL_ISH_AS_MASTER              GET_VALUE_C&gt;GET_VALUE_C&lt;/&gt; kann ein</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>Wert zu einer &lt;DS:CL.CL_ISH_ASM_PARAM_C&gt;mandantabhängigen,</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>zeitunabhängigen Applikationseinstellung&lt;/&gt; direkt ermittelt werden.</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>/</TDFORMAT>
       <TDLINE>Über die Methode</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>&lt;DS:CO.CL_ISH_AS_MASTER              SET_VALUE_C&gt;SET_VALUE_C&lt;/&gt; kann ein</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>Wert zu einer mandantabhängigen, zeitunabhängigen</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>Applikationseinstellung direkt verändert werden.</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>AS</TDFORMAT>
       <TDLINE>Durch Aufruf der Methode</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>&lt;DS:CO.CL_ISH_AS_MASTER              SET_VALUE_C&gt;SAVE&lt;/&gt; werden</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>sämtliche geänderten Applikationseinstellungen gespeichert.</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>U1</TDFORMAT>
