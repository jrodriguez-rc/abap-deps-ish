    </TLINE>
   </LINES>
   <I18N_LINES>
    <item>
     <LANGUAGE>D</LANGUAGE>
     <LINES>
      <TLINE>
       <TDFORMAT>U1</TDFORMAT>
       <TDLINE>&amp;MEANING&amp;</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>AS</TDFORMAT>
       <TDLINE>Das Interface IF_ISH_DESTROYABLE definiert die Schnittstelle eines</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>zerstörbaren Objekts.</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>AS</TDFORMAT>
       <TDLINE>Zerstören eines Objekts bedeutet, daß sämtliche Attribute initialisiert</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>werden.</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>/</TDFORMAT>
       <TDLINE>Außerdem werden sämtliche Ereignisbehandler deaktiviert.</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>/</TDFORMAT>
       <TDLINE>Das Objekt ist nach der Zerstörung nicht mehr verwendbar.</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>UT</TDFORMAT>
       <TDLINE>Methoden</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>B1</TDFORMAT>
       <TDLINE>DESTROY</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>AL</TDFORMAT>
       <TDLINE>Zerstört das Objekt.</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>AL</TDFORMAT>
       <TDLINE>Die Methode returniert ABAP_TRUE, wenn das Objekt erfolgreich zerstört</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>wurde.</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>B1</TDFORMAT>
       <TDLINE>IS_DESTROYED</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>AL</TDFORMAT>
       <TDLINE>Returniert ob das Objekt zerstört ist.</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>B1</TDFORMAT>
       <TDLINE>IS_IN_DESTROY_MODE</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>AL</TDFORMAT>
       <TDLINE>Returniert ob das Objekt gerade zerstört wird.</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>UT</TDFORMAT>
       <TDLINE>Ereignisse</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>B1</TDFORMAT>
       <TDLINE>EV_AFTER_DESTROY</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>AL</TDFORMAT>
       <TDLINE>Dieses Ereignis wird nach der Zerstörung des Objekts ausgelöst.</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>B1</TDFORMAT>
       <TDLINE>EV_BEFORE_DESTROY</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>AL</TDFORMAT>
       <TDLINE>Dieses Ereignis wird vor der Zerstörung des Objekts ausgelöst.</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>U1</TDFORMAT>
       <TDLINE>&amp;USE&amp;</TDLINE>
      </TLINE>
      <TLINE>
