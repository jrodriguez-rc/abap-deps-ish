     <LINES>
      <TLINE>
       <TDFORMAT>U1</TDFORMAT>
       <TDLINE>&amp;DEFINITION&amp;</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>AS</TDFORMAT>
       <TDLINE>Gibt an, dass die Daten in der Tabelle &lt;LS&gt;N1TC_BUFFER&lt;/&gt; nicht</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE> automatisch gelöscht werden.,,</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>U1</TDFORMAT>
       <TDLINE>&amp;USE&amp;</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>AS</TDFORMAT>
       <TDLINE>Sie können den Parameter verwenden, um die Systemgeschwindigkeit bei</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>aktivierter Prüfung des Behandlungsauftrags zu verbessern.</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>U1</TDFORMAT>
       <TDLINE>&amp;RECOMMENDATION&amp;</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>AS</TDFORMAT>
       <TDLINE>&lt;ZH&gt;Beachten Sie, dass die Tabelle &lt;LS&gt;N1TC_BUFFER&lt;/&gt; regelmäßig geleert</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>werden muss.&lt;/&gt; Verwenden Sie dazu den Report</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>=</TDFORMAT>
       <TDLINE> &lt;DS:RE.RN1TC_CLEARBUFFER_BDC&gt;RN1TC_CLEARBUFFER_BDC&lt;/&gt;.</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>U1</TDFORMAT>
       <TDLINE>&amp;DEPENDENCIES&amp;</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>AS</TDFORMAT>
       <TDLINE>Das System prüft diesen Parameter gemeinsam mit dem Benutzerparameter</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>&lt;LS&gt;EIN&lt;/&gt;.</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>U1</TDFORMAT>
       <TDLINE>&amp;EXAMPLE&amp;</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>AS</TDFORMAT>
      </TLINE>
     </LINES>
