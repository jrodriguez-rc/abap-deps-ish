      <TLINE>
       <TDFORMAT>AS</TDFORMAT>
       <TDLINE>MIt der Berechtigungsstufe wird gesteuert, welcher Benutzerkreis auf</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>ein Dokument zugreifen darf:</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>AS</TDFORMAT>
       <TDLINE>  0:   frei zugängliche innerhalb der Einrichtung</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>AS</TDFORMAT>
       <TDLINE>  1-7: differenzierter Zgriffsberechtigung der Mitarbeiter der OE</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>AS</TDFORMAT>
       <TDLINE>  8:   privates Dokument, für die eingetragen Benutzer</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>AS</TDFORMAT>
