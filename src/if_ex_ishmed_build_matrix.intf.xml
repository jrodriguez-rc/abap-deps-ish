     <CMPNAME>CREATE_MATRIX_FOR_USER</CMPNAME>
     <LANGU>E</LANGU>
     <DESCRIPT>Create Matrix in DB Table Buffer for a User</DESCRIPT>
    </SEOCOMPOTX>
   </DESCRIPTIONS>
