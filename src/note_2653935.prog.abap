FORM update_01. " TABD N1TC_MATRIX_B_IN ( DE , source: EV8 )

  DATA: lt_dd36  TYPE TABLE OF dd36m, ls_dd36 TYPE dd36m,
        lt_dd05m TYPE TABLE OF dd05m, ls_dd05m TYPE dd05m,
        ls_dd08v TYPE dd08v.

  CALL METHOD lcl_wb=>create_table     "#EC *
    EXPORTING
      i_tabname    = 'N1TC_MATRIX_B_IN'
      i_devclass   = 'N1BASE'
      i_exclass    = '1'
      i_authclass  = '00'
      i_tabclass   = 'TRANSP'
