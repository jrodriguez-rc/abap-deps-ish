   <I18N_LINES>
    <item>
     <LANGUAGE>D</LANGUAGE>
     <LINES>
      <TLINE>
       <TDFORMAT>U1</TDFORMAT>
       <TDLINE>&amp;MEANING&amp;</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>AS</TDFORMAT>
       <TDLINE>Das Interface &lt;ZH&gt;IF_ISH_POPUP_CALLBACK &lt;/&gt;definiert die Schnittstelle</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>zu Objekten, welche in einem&lt;ZH&gt; &lt;DS:IF.IF_ISH_PRC_POPUP&gt;Popup Prozess</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>=</TDFORMAT>
       <TDLINE>&lt;/&gt;&lt;/&gt; eingebunden werden.</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>AS</TDFORMAT>
       <TDLINE>Bei der Initialisierung eines Popup Prozesses kann dem Popup Prozess ein</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>Callback-Objekt übergeben werden.</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>/</TDFORMAT>
       <TDLINE>Dieses Callback-Objekt wird während der Ausführung des Popup Prozesses</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>zu bestimmten Zeitpunkten eingebunden.</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>AS</TDFORMAT>
       <TDLINE>Siehe dazu &lt;DS:IF.IF_ISH_PRC_POPUP&gt;IF_ISH_PRC_POPUP&lt;/&gt;.</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>U1</TDFORMAT>
