      <TDFORM>S_DOCU_SHOW</TDFORM>
      <TDSTYLE>S_DOCUS1</TDSTYLE>
      <TDVERSION>00004</TDVERSION>
      <TDLINESIZE>072</TDLINESIZE>
     </HEAD>
     <LINES>
      <TLINE>
       <TDFORMAT>U1</TDFORMAT>
       <TDLINE>&amp;DEFINITION&amp;</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>AS</TDFORMAT>
       <TDLINE>Gültigkeitsende des Klinischen Anwendungsbausteins.</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>U1</TDFORMAT>
       <TDLINE>&amp;USE&amp;</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>AS</TDFORMAT>
       <TDLINE>Klinische Anwendungsbausteine können Sie nur innerhalb ihres</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>Gültigkeitszeitraumes in klinische Auftragstypen definieren.</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>AS</TDFORMAT>
       <TDLINE>Die Integration in bereits definierten Auftragstypen bleibt auch</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>ausserhalb der Gültigkeit erhalten. Das heisst, um die weitere Nutzung</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>der Funktionalität eines Bausteines zu unterbinden, müssen Sie die</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>implementierenden Auftragstypen deaktivieren.</TDLINE>
      </TLINE>
      <TLINE/>
      <TLINE>
       <TDFORMAT>/:</TDFORMAT>
       <TDLINE>INCLUDE ISH_CORD_PREREG_DEF OBJECT DOKU ID TX</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>U1</TDFORMAT>
       <TDLINE>&amp;DEPENDENCIES&amp;</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>AS</TDFORMAT>
      </TLINE>
      <TLINE>
       <TDFORMAT>U1</TDFORMAT>
       <TDLINE>&amp;EXAMPLE&amp;</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>AS</TDFORMAT>
      </TLINE>
     </LINES>
    </item>
