    <TLINE>
     <TDLINE>functionality for configurations of &lt;DS:IF.IF_ISH_SCR_SPLITTER&gt;splitter</TDLINE>
    </TLINE>
    <TLINE>
     <TDLINE>screens&lt;/&gt;.</TDLINE>
    </TLINE>
    <TLINE>
     <TDFORMAT>AS</TDFORMAT>
     <TDLINE>This interface contains the interface</TDLINE>
