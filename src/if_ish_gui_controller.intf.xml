    </TLINE>
    <TLINE>
     <TDFORMAT>B1</TDFORMAT>
     <TDLINE>GET_PARENT_CONTROLLER</TDLINE>
    </TLINE>
    <TLINE>
     <TDFORMAT>AL</TDFORMAT>
     <TDLINE>Returns the parent control object.</TDLINE>
    </TLINE>
    <TLINE>
     <TDFORMAT>B1</TDFORMAT>
     <TDLINE>GET_VCODE</TDLINE>
    </TLINE>
    <TLINE>
     <TDFORMAT>AL</TDFORMAT>
