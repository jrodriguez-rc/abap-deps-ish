    </TLINE>
    <TLINE>
     <TDLINE>&lt;DS:IF.IF_ISH_GUI_ELEMENT&gt;IF_ISH_GUI_ELEMENT&lt;/&gt;.</TDLINE>
    </TLINE>
    <TLINE>
     <TDFORMAT>AS</TDFORMAT>
     <TDLINE>The interface IF_ISH_GUI_CONTROLLER contains the interface</TDLINE>
    </TLINE>
    <TLINE>
     <TDLINE>&lt;DS:IF.IF_ISH_DESTROYABLE&gt;IF_ISH_DESTROYABLE&lt;/&gt;.</TDLINE>
    </TLINE>
    <TLINE>
