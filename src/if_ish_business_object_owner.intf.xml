   <I18N_LINES>
    <item>
     <LANGUAGE>D</LANGUAGE>
     <LINES>
      <TLINE>
       <TDFORMAT>U1</TDFORMAT>
       <TDLINE>&amp;MEANING&amp;</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>AS</TDFORMAT>
       <TDLINE>Das Interface IF_ISH_BUSINESS_OBJECT_OWNER definiert die Schnittstelle</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>eines Eigentümers eines &lt;DS:IF.IF_ISH_BUSINESS_OBJECT&gt;Geschäftsobjekts</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>=</TDFORMAT>
       <TDLINE>&lt;/&gt;.</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>AS</TDFORMAT>
       <TDLINE>Ein Geschäftsobjekt kann mehrere Eigentümer haben.</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>AS</TDFORMAT>
       <TDLINE>Geschäftsobjekte beziehen deren Eigentümer bei verschiedenen</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>Funktionalitäten in die Verarbeitung ein (Callback-Mechanismus). Der</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>Eigentümer hat somit die Möglichkeit, bestimmte Funktionalitäten zu</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>erlauben oder zu verbieten.</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>AS</TDFORMAT>
       <TDLINE>Das Interface IF_ISH_BUSINESS_OBJECT_OWNER umfaßt das Interface</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>&lt;DS:IF.IF_ISH_CB_DESTROYABLE&gt;IF_ISH_CB_DESTROYABLE&lt;/&gt;.</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>UT</TDFORMAT>
       <TDLINE>Methoden:</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>B1</TDFORMAT>
       <TDLINE>CB_AFTER_COMMIT</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>AL</TDFORMAT>
       <TDLINE>Diese Methode wird vom Geschäftsobjekt nach COMMIT WORK aufgerufen.</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>B1</TDFORMAT>
       <TDLINE>CB_AFTER_ROLLBACK</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>AL</TDFORMAT>
       <TDLINE>Diese Methode wird vom Geschäftsobjekt nach ROLLBACK WORK aufgerufen.</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>B1</TDFORMAT>
       <TDLINE>CB_CLEAR_ALL_SNAPSHOTS</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>AL</TDFORMAT>
       <TDLINE>Diese Methode wird vom Geschäftsobjekt aufgerufen, bevor alle</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>Speicherabzüge gelöscht werden.</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>B1</TDFORMAT>
       <TDLINE>CB_CLEAR_SNAPSHOT</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>AL</TDFORMAT>
       <TDLINE>Diese Methode wird vom Geschäftsobjekt aufgerufen, bevor ein</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>Speicherabzug gelöscht wird.</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>B1</TDFORMAT>
       <TDLINE>CB_CONSTRUCTION</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>AL</TDFORMAT>
       <TDLINE>Diese Methode wird vom Geschäftsobjekt zum Zeitpunkt der Instanzierung</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>aufgerufen.</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>B1</TDFORMAT>
       <TDLINE>CB_DEREGISTER_OWNER</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>AL</TDFORMAT>
       <TDLINE>Diese Methode wird vom Geschäftsobjekt aufgerufen, bevor ein Eigentümer</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>deregistriert wird.</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>B1</TDFORMAT>
       <TDLINE>CB_MARK_FOR_DELETION</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>AL</TDFORMAT>
       <TDLINE>Diese Methode wird vom Geschäftsobjekt aufgerufen, bevor es zur Löschung</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>markiert wird.</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>B1</TDFORMAT>
       <TDLINE>CB_READONLY</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>AL</TDFORMAT>
       <TDLINE>Diese Methode wird vom Geschäftsobjekt aufgerufen um festzustellen, ob</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>es nur lesbar ist.</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>AL</TDFORMAT>
       <TDLINE>Hinweis: Diese Methode wird vom Geschäftsobjekt nicht aufgerufen, wenn</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>das Geschäftsobjekt an sich bereits nur lesbar ist.</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>B1</TDFORMAT>
       <TDLINE>CB_REGISTER_OWNER</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>AL</TDFORMAT>
       <TDLINE>Diese Methode wird vom Geschäftsobjekt aufgerufen, bevor ein Eigentümer</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>registriert wird.</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>B1</TDFORMAT>
       <TDLINE>CB_RELOAD</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>AL</TDFORMAT>
       <TDLINE>Diese Methode wird vom Geschäftsobjekt vor dem erneuten Laden aus der</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>Datenbank aufgerufen.</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>B1</TDFORMAT>
       <TDLINE>CB_RESET</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>AL</TDFORMAT>
       <TDLINE>Diese Methode wird vom Geschäftsobjekt vor dem Zurücksetzen aufgerufen.</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>B1</TDFORMAT>
       <TDLINE>CB_SAVE</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>AL</TDFORMAT>
       <TDLINE>Diese Methode wird vor dem Sichern eines Geschäftsobjekts aufgerufen.</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>B1</TDFORMAT>
       <TDLINE>CB_SNAPSHOT</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>AL</TDFORMAT>
       <TDLINE>Diese Methode wird vom Geschäftsobjekt aufgerufen, bevor ein</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>Speicherabzug gemacht wird.</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>B1</TDFORMAT>
       <TDLINE>CB_TOUCH</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>AL</TDFORMAT>
       <TDLINE>Diese Methode wird vom Geschäftsobjekt aufgerufen, bevor es berührt</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>wird.</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>B1</TDFORMAT>
       <TDLINE>CB_UNDO</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>AL</TDFORMAT>
       <TDLINE>Diese Methode wird vom Geschäftsobjekt aufgerufen, bevor ein zuvor</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>gesicherter Speicherabzug wiederhergestellt wird.</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>B1</TDFORMAT>
       <TDLINE>CB_UNTOUCH</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>AL</TDFORMAT>
       <TDLINE>Diese Methode wird vom Geschäftsobjekt aufgerufen, bevor die Berührung</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>rückgängig gemacht wird.</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>U1</TDFORMAT>
       <TDLINE>&amp;USE&amp;</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>AS</TDFORMAT>
      </TLINE>
      <TLINE>
       <TDFORMAT>U1</TDFORMAT>
       <TDLINE>&amp;EXAMPLE&amp;</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>AS</TDFORMAT>
      </TLINE>
      <TLINE>
       <TDFORMAT>U1</TDFORMAT>
       <TDLINE>&amp;HINTS&amp;</TDLINE>
      </TLINE>
      <TLINE>
