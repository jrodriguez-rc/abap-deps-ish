     <LINES>
      <TLINE>
       <TDFORMAT>U1</TDFORMAT>
       <TDLINE>&amp;DEFINITION&amp;</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>AS</TDFORMAT>
       <TDLINE>Mit dem Verlassen einer OE bzw. Entlassung eines Patienten endet der</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>Behandlungsauftrag.</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>AS</TDFORMAT>
       <TDLINE>Die Nachlaufzeit gibt an, wievieleTage darüber hinaus der</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>Behandlungsauftrag verlängert wird, damit Nacharbeiten an den</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>Patientendaten möglich sind.</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>AS</TDFORMAT>
       <TDLINE>Die Nachlaufzeit kann nach Bedarf eines Projektes stufenweise</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>konfiguriert werden: von einem Wert der einrichtungsweit gilt, bis hin</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>zu auf OE-Konstellationen abgestimmte Zeiten.</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>U1</TDFORMAT>
       <TDLINE>&amp;USE&amp;</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>AS</TDFORMAT>
      </TLINE>
      <TLINE>
       <TDFORMAT>U1</TDFORMAT>
