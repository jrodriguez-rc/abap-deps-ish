      <TDFORM>S_DOCU_SHOW</TDFORM>
      <TDSTYLE>S_DOCUS1</TDSTYLE>
      <TDVERSION>00004</TDVERSION>
      <TDLINESIZE>072</TDLINESIZE>
     </HEAD>
     <LINES>
      <TLINE>
       <TDFORMAT>U1</TDFORMAT>
       <TDLINE>&amp;DEFINITION&amp;</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>AS</TDFORMAT>
       <TDLINE>Das Feld &quot;Gewünschtes Jahr&quot; enthält die Jahreszahl, wann eine</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>Behandlung stattfinden soll.</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>AS</TDFORMAT>
       <TDLINE>Verwendung bei Vormerkungen: Zusammen mit einem Monat und einer</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>&quot;Circa-Angabe&quot; (Zeitpunktinterpretation) kann ein Zeitbereich angegeben</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>werden, innerhalb dessen ein Behandlungs- oder OP-Termin gewünscht</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>wird.</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>AS</TDFORMAT>
       <TDLINE>Beispiel: Anfang Oktober 1998.</TDLINE>
      </TLINE>
     </LINES>
    </item>
