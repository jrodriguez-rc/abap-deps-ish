 <asx:abap xmlns:asx="http://www.sap.com/abapxml" version="1.0">
  <asx:values>
   <LONGTEXTS>
    <item>
     <DOKIL>
      <ID>HY</ID>
      <OBJECT>SIMGISH_TN1TC_CATEGORY</OBJECT>
      <LANGU>D</LANGU>
      <TYP>E</TYP>
      <VERSION>0003</VERSION>
      <MASTERLANG>X</MASTERLANG>
      <TXTLINES>00021</TXTLINES>
      <DOKSTATE>A</DOKSTATE>
     </DOKIL>
     <HEAD>
      <TDOBJECT>DSYS</TDOBJECT>
      <TDNAME>SIMGISH_TN1TC_CATEGORY</TDNAME>
      <TDID>HY</TDID>
      <TDSPRAS>D</TDSPRAS>
      <TDFORM>S_DOCU_SHOW</TDFORM>
      <TDSTYLE>S_DOCUS1</TDSTYLE>
      <TDVERSION>00003</TDVERSION>
      <TDLINESIZE>072</TDLINESIZE>
     </HEAD>
     <LINES>
      <TLINE>
       <TDFORMAT>UT</TDFORMAT>
       <TDLINE>&amp;USE&amp;</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>AS</TDFORMAT>
       <TDLINE>Mit dieser Customizing-Aktivität bearbeiten Sie die Texte, die das</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>System für die Begründungskategorien bei der Beantragung eines</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>temporären Behandlungsauftrags verwendet.</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>AS</TDFORMAT>
       <TDLINE>Das System zeigt dem Benutzer die Standardtexte in dem Dialog</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>&lt;LS&gt;Beantragung eines Behandlungsauftrags &lt;/&gt;an. Sie können für eine</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>Einrichtung bis zu 10 Standardtexte definieren.</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>UT</TDFORMAT>
       <TDLINE>&amp;PRECONDITIONS&amp;</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>AS</TDFORMAT>
      </TLINE>
      <TLINE>
       <TDFORMAT>UT</TDFORMAT>
       <TDLINE>&amp;STANDARD_SETUP&amp;</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>AS</TDFORMAT>
      </TLINE>
      <TLINE>
       <TDFORMAT>UT</TDFORMAT>
       <TDLINE>&amp;ACTIVITY&amp;</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>AS</TDFORMAT>
       <TDLINE>Führen Sie folgende Schritte aus:</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>B1</TDFORMAT>
       <TDLINE>Tragen Sie die Standardtexte in das Feld &lt;ZK&gt;Begründungskategorie&lt;/&gt;</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>ein.</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>B1</TDFORMAT>
       <TDLINE>Geben Sie im Feld &lt;ZK&gt;&lt;LS&gt;Reihenfolge der Begründungskategorien&lt;/&gt;&lt;/&gt;</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>pro Text eine Ordnungszahl ein. Dadurch definieren Sie die Position der</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>Texte in der Auswahlliste &lt;LS&gt;Kategorie &lt;/&gt;des Dialogs &lt;LS&gt;Beantragung</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>eines Behandlungsauftrags.&lt;/&gt;</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>B1</TDFORMAT>
       <TDLINE>Wählen Sie zusätzlich im Feld &lt;ZK&gt;Einstellungen für die Begründung&lt;/&gt;</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>aus, ob der Benutzer bei dieser Kategorie einen zusätzlichen Freitext</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>erfassen kann bzw. muss.</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>UT</TDFORMAT>
       <TDLINE>&amp;EXAMPLE&amp;</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>AS</TDFORMAT>
      </TLINE>
     </LINES>
    </item>
    <item>
     <DOKIL>
      <ID>HY</ID>
      <OBJECT>SIMGISH_TN1TC_CATEGORY</OBJECT>
      <LANGU>E</LANGU>
      <TYP>E</TYP>
      <VERSION>0003</VERSION>
      <TXTLINES>00013</TXTLINES>
      <DOKSTATE>A</DOKSTATE>
     </DOKIL>
     <HEAD>
      <TDOBJECT>DSYS</TDOBJECT>
      <TDNAME>SIMGISH_TN1TC_CATEGORY</TDNAME>
      <TDID>HY</TDID>
      <TDSPRAS>E</TDSPRAS>
      <TDFORM>S_DOCU_SHOW</TDFORM>
      <TDSTYLE>S_DOCUS1</TDSTYLE>
      <TDVERSION>00003</TDVERSION>
      <TDLINESIZE>072</TDLINESIZE>
     </HEAD>
     <LINES>
      <TLINE>
       <TDFORMAT>UT</TDFORMAT>
       <TDLINE>&amp;USE&amp;</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>AS</TDFORMAT>
       <TDLINE>In this Customizing activity, you edit the texts the system uses for the</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>reason categories when requesting a temporary treatment authorization.</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>AS</TDFORMAT>
       <TDLINE>The system displays the standard texts in the &lt;LS&gt;Request Treatment</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>Authorization&lt;/&gt; dialog. You can define up to 10 standard texts for an</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>institution.</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>UT</TDFORMAT>
       <TDLINE>&amp;PRECONDITIONS&amp;</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>AS</TDFORMAT>
      </TLINE>
      <TLINE>
       <TDFORMAT>UT</TDFORMAT>
       <TDLINE>&amp;STANDARD_SETUP&amp;</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>AS</TDFORMAT>
      </TLINE>
      <TLINE>
       <TDFORMAT>UT</TDFORMAT>
       <TDLINE>&amp;ACTIVITY&amp;</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>AS</TDFORMAT>
       <TDLINE>Carry out the following steps:</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>B1</TDFORMAT>
       <TDLINE>Enter the standard texts in the &lt;ZK&gt;Reason Category&lt;/&gt; field.</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>B1</TDFORMAT>
       <TDLINE>Enter an ordinal number for each text in the &lt;LS&gt;Sequence of Reason</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>Categories&lt;/&gt; field. This defines the position of the texts in the &lt;LS&gt;</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>=</TDFORMAT>
       <TDLINE>Category&lt;/&gt; selection list in the &lt;LS&gt;Request Treatment Authorization&lt;/&gt;</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>=</TDFORMAT>
       <TDLINE> dialog.</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>B1</TDFORMAT>
       <TDLINE>In the &lt;ZK&gt;Settings for Reason&lt;/&gt; field, specify whether the user can or</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>must enter an additional free text for this category.</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>UT</TDFORMAT>
       <TDLINE>&amp;EXAMPLE&amp;</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>AS</TDFORMAT>
      </TLINE>
     </LINES>
    </item>
    <item>
     <DOKIL>
      <ID>HY</ID>
      <OBJECT>SIMGISH_TN1TC_CATEGORY</OBJECT>
      <LANGU>S</LANGU>
      <TYP>E</TYP>
      <VERSION>0003</VERSION>
      <TXTLINES>00015</TXTLINES>
      <DOKSTATE>A</DOKSTATE>
     </DOKIL>
     <HEAD>
      <TDOBJECT>DSYS</TDOBJECT>
      <TDNAME>SIMGISH_TN1TC_CATEGORY</TDNAME>
      <TDID>HY</TDID>
      <TDSPRAS>S</TDSPRAS>
      <TDFORM>S_DOCU_SHOW</TDFORM>
      <TDSTYLE>S_DOCUS1</TDSTYLE>
      <TDVERSION>00003</TDVERSION>
      <TDLINESIZE>072</TDLINESIZE>
     </HEAD>
     <LINES>
      <TLINE>
       <TDFORMAT>UT</TDFORMAT>
       <TDLINE>&amp;USE&amp;</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>AS</TDFORMAT>
       <TDLINE>Con esta actividad de Customizing trate los textos que utiliza el</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>sistema para las categorías de motivo durante la solicitud de una</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>autorización de tratamiento temporal.</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>AS</TDFORMAT>
       <TDLINE>El sistema muestra al usuario los textos estándar en la ventana de</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>diálogo &lt;LS&gt;Solicitud de una autorización de tratamiento&lt;/&gt;. Para un</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>centro sanitario puede definir hasta 10 textos estándar.</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>UT</TDFORMAT>
       <TDLINE>&amp;PRECONDITIONS&amp;</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>AS</TDFORMAT>
      </TLINE>
      <TLINE>
       <TDFORMAT>UT</TDFORMAT>
       <TDLINE>&amp;STANDARD_SETUP&amp;</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>AS</TDFORMAT>
      </TLINE>
      <TLINE>
       <TDFORMAT>UT</TDFORMAT>
       <TDLINE>&amp;ACTIVITY&amp;</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>AS</TDFORMAT>
       <TDLINE>Realice los siguientes pasos:</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>B1</TDFORMAT>
       <TDLINE>Introduzca los textos estándar en el campo &lt;ZK&gt;Categoría de motivo&lt;/&gt;.</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>B1</TDFORMAT>
       <TDLINE>Introduzca en el campo &lt;ZK&gt;&lt;LS&gt;Secuencia de categorías de motivo&lt;/&gt;&lt;/&gt;</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>por texto un número de secuencia. De este modo se define la posición de</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>los textos en la lista de selección &lt;LS&gt;Categoría&lt;/&gt; del diálogo &lt;LS&gt;</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>=</TDFORMAT>
       <TDLINE>Solicitud de una autorización de tratamiento&lt;/&gt;.</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>B1</TDFORMAT>
       <TDLINE>Seleccione además en el campo &lt;ZK&gt;Parametrizaciones para el motivo&lt;/&gt; si</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>el usuario puede registrar en esta categoría un texto libre adicional o</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>debe seguir las categorías de motivo.</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>UT</TDFORMAT>
       <TDLINE>&amp;EXAMPLE&amp;</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>AS</TDFORMAT>
      </TLINE>
     </LINES>
    </item>
   </LONGTEXTS>
  </asx:values>
 </asx:abap>
