     <LINES>
      <TLINE>
       <TDFORMAT>U1</TDFORMAT>
       <TDLINE>&amp;DEFINITION&amp;</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>AS</TDFORMAT>
       <TDLINE>Bei der Prüfung auf das Vorliegen eines Behandlungsauftrags wird für den</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>Benutzer geprüft, für welche OE-Konstellationen dieser zuständig ist.</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>Diese OE-Konstellationen werden unter dem Begriff OE-Zuständigkeit</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>zusammengefasst und sind paarweise definiert: ein Fachbereich mit einer</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>Station (pflegerische OE) oder ein Fachbereich mit einer Leistungstelle</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>/ Ambulanz. Dadurch werden auch sogenannte interdisziplinäre</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>OE-Konstellationen mit berücksichtigt.</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>AS</TDFORMAT>
       <TDLINE>OE-Zuständigkeiten werden im Customizing mit Rollen verknüpft und auf</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>diesem Weg dem Benutzer zugewiesen.</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>U1</TDFORMAT>
       <TDLINE>&amp;USE&amp;</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>AS</TDFORMAT>
      </TLINE>
      <TLINE>
       <TDFORMAT>U1</TDFORMAT>
       <TDLINE>&amp;DEPENDENCIES&amp;</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>AS</TDFORMAT>
      </TLINE>
      <TLINE>
       <TDFORMAT>U1</TDFORMAT>
       <TDLINE>&amp;EXAMPLE&amp;</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>AS</TDFORMAT>
      </TLINE>
     </LINES>
    </item>
