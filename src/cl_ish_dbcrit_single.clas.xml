    <item>
     <LANGUAGE>D</LANGUAGE>
     <LINES>
      <TLINE>
       <TDFORMAT>U1</TDFORMAT>
       <TDLINE>&amp;FUNCTIONALITY&amp;</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>AS</TDFORMAT>
       <TDLINE>Die Klasse CL_ISH_DBCRIT_SINGLE dient zur Darstellung eines Einzelwertes</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>als Selektionskriterium für Datenbankzugriffe.</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>AS</TDFORMAT>
       <TDLINE>Jedes Einzelwertkriterium enthält einen Feldnamen, einen Operator und</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>einen Wert.</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>AS</TDFORMAT>
       <TDLINE>Die möglichen Operatoren sind:</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>B1</TDFORMAT>
       <TDLINE>EQ (Gleich)</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>/</TDFORMAT>
       <TDLINE>Definiert durch die Konstante CO_OPERATOR_EQ.</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>B1</TDFORMAT>
       <TDLINE>NE (Ungleich)</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>/</TDFORMAT>
       <TDLINE>Definiert durch die Konstante CO_OPERATOR_NE.</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>B1</TDFORMAT>
       <TDLINE>GT (Grösser als)</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>/</TDFORMAT>
       <TDLINE>Definiert durch die Konstante CO_OPERATOR_GT.</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>B1</TDFORMAT>
       <TDLINE>GE (Grösser oder gleich)</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>/</TDFORMAT>
       <TDLINE>Definiert durch die Konstante CO_OPERATOR_GE.</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>B1</TDFORMAT>
       <TDLINE>LT (Kleiner als)</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>/</TDFORMAT>
       <TDLINE>Definiert durch die Konstante CO_OPERATOR_LT.</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>B1</TDFORMAT>
       <TDLINE>LE (Kleiner oder gleich)</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>/</TDFORMAT>
       <TDLINE>Definiert durch die Konstante CO_OPERATOR_LE.</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>B1</TDFORMAT>
       <TDLINE>NULL</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>/</TDFORMAT>
       <TDLINE>Entspricht der WHERE-Klausel &apos;&lt;Feldname&gt; IS NULL&apos;.</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>/</TDFORMAT>
       <TDLINE>Definiert durch die Konstante CO_OPERATOR_NULL.</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>B1</TDFORMAT>
       <TDLINE>NOT_NULL</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>/</TDFORMAT>
       <TDLINE>Entspricht der WHERE-Klausel &apos;&lt;Feldname&gt; IS NOT NULL&apos;.</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>/</TDFORMAT>
       <TDLINE>Definiert durch die Konstante CO_OPERATOR_NOT_NULL.</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>AS</TDFORMAT>
       <TDLINE>Die Methode &lt;DS:CO.CL_ISH_DBCRITERION            AS_STRING&gt;AS_STRING&lt;/&gt;</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>eines Einzelwertkriteriums liefert folgenden Wert:</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>B1</TDFORMAT>
       <TDLINE>Bei Operator NULL: ( &lt;Feldname&gt; IS NULL )</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>B1</TDFORMAT>
       <TDLINE>Bei Operator NOT_NULL: ( &lt;Feldname&gt; IS NOT NULL )</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>B1</TDFORMAT>
       <TDLINE>Bei allen anderen Operatoren: ( &lt;Feldname&gt; &lt;Operator&gt; &lt;(&gt;</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>=</TDFORMAT>
       <TDLINE>&lt;&lt;)&gt;Wert&gt; )</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>AS</TDFORMAT>
       <TDLINE>Die Instanzierung eines Einzelwertkriteriums kann entweder direkt über</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>die CREATE OBJECT Anweisung erfolgen (siehe Methode</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>&lt;DS:CO.CL_ISH_DBCRIT_SINGLE          CONSTRUCTOR&gt;CONSTRUCTOR&lt;/&gt;) oder</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>durch Aufruf einer der statischen CREATE* Methoden.</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>U1</TDFORMAT>
       <TDLINE>&amp;RELATIONS&amp;</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>AS</TDFORMAT>
       <TDLINE>Die Klasse CL_ISH_DBCRIT_SINGLE erbt von der Klasse</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>&lt;DS:CL.CL_ISH_DBCRITERION&gt;CL_ISH_DBCRITERION&lt;/&gt;.</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>U1</TDFORMAT>
       <TDLINE>&amp;EXAMPLE&amp;</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>AS</TDFORMAT>
      </TLINE>
      <TLINE>
       <TDFORMAT>U1</TDFORMAT>
       <TDLINE>&amp;HINTS&amp;</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>AS</TDFORMAT>
      </TLINE>
      <TLINE>
       <TDFORMAT>U1</TDFORMAT>
       <TDLINE>&amp;FURTHER_SOURCES_OF_INF&amp;</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>AS</TDFORMAT>
      </TLINE>
     </LINES>
