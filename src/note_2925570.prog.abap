      dp_invalid_parameter = 1
      dp_error_general     = 2
      cntl_error           = 3
      OTHERS               = 4.
  CHECK sy-subrc EQ 0.

  CHECK gv_unit_test IS INITIAL.

  CALL METHOD g_html_viewer->('SHOW_URL')
    EXPORTING
