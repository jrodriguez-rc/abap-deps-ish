     </DOKIL>
     <HEAD>
      <TDOBJECT>DSYS</TDOBJECT>
      <TDNAME>SIMGISH_GUI_A_DRWEVENT</TDNAME>
      <TDID>HY</TDID>
      <TDSPRAS>D</TDSPRAS>
      <TDFORM>S_DOCU_SHOW</TDFORM>
      <TDSTYLE>S_DOCUS1</TDSTYLE>
      <TDVERSION>00002</TDVERSION>
      <TDLINESIZE>072</TDLINESIZE>
     </HEAD>
     <LINES>
      <TLINE>
       <TDFORMAT>UT</TDFORMAT>
       <TDLINE>&amp;USE&amp;</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>AS</TDFORMAT>
       <TDLINE>Mit diesem &lt;DS:GLOS.5C733369F735D311AE620060B03C9BBE&gt;Business Add-In</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>(BAdI)&lt;/&gt; können Sie nach der Verarbeitung eines Schubladenereignisses</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>kundenspezifische Verarbeitungen vornehmen.</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>UT</TDFORMAT>
       <TDLINE>&amp;PRECONDITIONS&amp;</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>UT</TDFORMAT>
       <TDLINE>&amp;STANDARD_SETUP&amp;</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>AS</TDFORMAT>
       <TDLINE>Im Standard ist keine BAdI-Implementierung aktiv.</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>*</TDFORMAT>
       <TDLINE>Das BAdI ist mehrfach nutzbar.</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>*</TDFORMAT>
       <TDLINE>Das BAdI ist filterabhängig. Als Filterkriterium verwenden Sie die</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>Identifikation einer Dialoganwendung (siehe Domäne N1GUI_BADIAPPLID).</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>UT</TDFORMAT>
       <TDLINE>&amp;ACTIVITY&amp;</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>*</TDFORMAT>
      </TLINE>
      <TLINE>
       <TDFORMAT>/:</TDFORMAT>
       <TDLINE>INCLUDE BADI_ENH_IMPLEMENTATION OBJECT DOKU ID TX</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>UT</TDFORMAT>
       <TDLINE>&amp;EXAMPLE&amp;</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>UT</TDFORMAT>
       <TDLINE>&amp;SEE_ALSO&amp;</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>*</TDFORMAT>
       <TDLINE>Dieses BAdI verwendet das Interface IF_EX_ ISH_GUI_A_DRWEVENT. Um</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>weitere Informationen zu erhalten, zeigen Sie das Interface im Class</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>Builder an.</TDLINE>
      </TLINE>
     </LINES>
    </item>
