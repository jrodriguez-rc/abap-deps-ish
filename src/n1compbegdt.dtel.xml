     <HEAD>
      <TDOBJECT>DOKU</TDOBJECT>
      <TDNAME>N1COMPBEGDT</TDNAME>
      <TDID>DE</TDID>
      <TDSPRAS>D</TDSPRAS>
      <TDFORM>S_DOCU_SHOW</TDFORM>
      <TDSTYLE>S_DOCUS1</TDSTYLE>
      <TDVERSION>00006</TDVERSION>
      <TDLINESIZE>072</TDLINESIZE>
     </HEAD>
     <LINES>
      <TLINE>
       <TDFORMAT>U1</TDFORMAT>
       <TDLINE>&amp;DEFINITION&amp;</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>AS</TDFORMAT>
       <TDLINE>Gültigkeitsbeginn des Bausteines</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>U1</TDFORMAT>
       <TDLINE>&amp;USE&amp;</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>AS</TDFORMAT>
       <TDLINE>Klinische Anwendungsbausteine können nur innerhalb ihres</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>Gültigkeitszeitraumes in klinische Auftragstypen definiert werden.</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>AS</TDFORMAT>
       <TDLINE>Die Integration in bereits definierten Auftragstypen bleibt auch</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>außerhalb der Gültigkeit erhalten. Das heißt, um die weitere Nutzung der</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>Funktionalität eines Bausteines zu unterbinden, müssen Sie die</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>implementierenden Auftragstypen deaktivieren.</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>AS</TDFORMAT>
       <TDLINE>Die Gültigkeit bezieht sich also nicht auf die Verwendbarkeit des</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>Bausteines auf Bewegungsebene sondern auf die Verwendbarkeit in der</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>Stammdatendefinition.</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>/:</TDFORMAT>
       <TDLINE>INCLUDE ISH_CORD_PREREG_DEF OBJECT DOKU ID TX</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>U1</TDFORMAT>
       <TDLINE>&amp;DEPENDENCIES&amp;</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>AS</TDFORMAT>
      </TLINE>
      <TLINE>
       <TDFORMAT>U1</TDFORMAT>
       <TDLINE>&amp;EXAMPLE&amp;</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>AS</TDFORMAT>
      </TLINE>
     </LINES>
    </item>
    <item>
     <DOKIL>
      <ID>DE</ID>
