      <TLINE>
       <TDFORMAT>AS</TDFORMAT>
       <TDLINE>Dieser Wert gibt an, ob die Daten einer Liste aktualisiert werden</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>sollen.</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>AS</TDFORMAT>
       <TDLINE>Wert:</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>AS</TDFORMAT>
       <TDLINE>0 = Nichts aktualisieren</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>AS</TDFORMAT>
       <TDLINE>1 = Ausgewählte Zeile(n) aktualisieren</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>AS</TDFORMAT>
       <TDLINE>2 = Alles (gesamte Liste) aktualisieren</TDLINE>
      </TLINE>
