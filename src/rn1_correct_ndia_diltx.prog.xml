     <LINES>
      <TLINE>
       <TDFORMAT>U1</TDFORMAT>
       <TDLINE>&amp;PURPOSE&amp;</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>AS</TDFORMAT>
       <TDLINE>Dieser Report ist ein Korrekturreport, der das fehlende</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>Langtextkennzeichen bei Diagnosen setzt, welche in der Diagnosen-/Lei</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>=</TDFORMAT>
       <TDLINE>stungsschnellerfassung mit einem Langtext erfasst wurden.</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>AS</TDFORMAT>
       <TDLINE>Sie können den Report auf einem System mehrfach ausführen. Sie können</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>den Report jederzeit auf Ihrem System ausführen. Beachten Sie, dass Sie</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>den Report durchführen müssen, wenn Sie das Klinische System mit Release</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>6.00 bereits vor dem Patch 27 im Einsatz haben. Wenn Sie das Klinische</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>System erst nach Patch 27 des Releases 6.00 im Einsatz haben, ist die</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>Durchführung des Reports nicht zwingend erforderlich.</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>U1</TDFORMAT>
       <TDLINE>&amp;INTEGRATION&amp;</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>AS</TDFORMAT>
      </TLINE>
      <TLINE>
       <TDFORMAT>U1</TDFORMAT>
       <TDLINE>&amp;PREREQUISITES&amp;</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>AS</TDFORMAT>
      </TLINE>
      <TLINE>
       <TDFORMAT>U1</TDFORMAT>
       <TDLINE>&amp;FEATURES&amp;</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>AS</TDFORMAT>
      </TLINE>
      <TLINE>
       <TDFORMAT>U2</TDFORMAT>
       <TDLINE>&amp;SELECTION&amp;</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>AS</TDFORMAT>
       <TDLINE>Wenn Sie &lt;LS&gt;Testlauf&lt;/&gt; markieren, führt der Report keine Änderungen</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>auf der Datenbank durch.</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>U2</TDFORMAT>
       <TDLINE>&amp;STANDARD_VARIANTS&amp;</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>AS</TDFORMAT>
      </TLINE>
      <TLINE>
       <TDFORMAT>U2</TDFORMAT>
       <TDLINE>&amp;OUTPUT&amp;</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>AS</TDFORMAT>
       <TDLINE>Der Report erstellt eine Liste mit den Diagnosen bei denen ein</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>Langtextkennzeichen nachgesetzt wurde.</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>UT</TDFORMAT>
       <TDLINE>&amp;ACTIVITIES&amp;</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>AS</TDFORMAT>
      </TLINE>
      <TLINE>
       <TDFORMAT>U1</TDFORMAT>
       <TDLINE>&amp;EXAMPLE&amp;</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>AS</TDFORMAT>
      </TLINE>
     </LINES>
