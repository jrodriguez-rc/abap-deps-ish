     <TDLINE>for the interface &lt;DS:IF.IF_ISH_BUSINESS_OBJECT&gt;IF_ISH_BUSINESS_OBJECT</TDLINE>
    </TLINE>
    <TLINE>
     <TDFORMAT>=</TDFORMAT>
     <TDLINE>&lt;/&gt;.</TDLINE>
    </TLINE>
