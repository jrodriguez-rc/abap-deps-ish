     <LINES>
      <TLINE>
       <TDFORMAT>U1</TDFORMAT>
       <TDLINE>&amp;FUNCTIONALITY&amp;</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>AS</TDFORMAT>
       <TDLINE>Die Klasse CL_ISH_BOSAVER dient zum Sichern oder Prüfen von</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>&lt;DS:IF.IF_ISH_BUSINESS_OBJECT&gt;Geschäftsobjekten&lt;/&gt;.</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>UT</TDFORMAT>
       <TDLINE>Methoden:</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>B1</TDFORMAT>
       <TDLINE>CB_*</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>AL</TDFORMAT>
       <TDLINE>Diese Methoden werden von den Geschäftsobjekten zu verschiedenen</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>Zeitpunkten gerufen.</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>AL</TDFORMAT>
       <TDLINE>Sie gewährleisten, dass manche Geschäftsobjekt-Methoden nur durch das</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>Sicherungsobjekt aufgerufen werden können (Callback-Mechanismus).</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>B1</TDFORMAT>
       <TDLINE>CHECK_HAS_BO</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>AL</TDFORMAT>
       <TDLINE>Diese Methode löst eine Ausnahme vom Typ</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>&lt;DS:CL.CX_ISH_STATIC_HANDLER&gt;CX_ISH_STATIC_HANDLER&lt;/&gt; aus, wenn ein</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>bestimmtes Geschäftsobjekt nicht in der Liste der zu verarbeitenden</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>Geschäftsobjekte enthalten ist.</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>B1</TDFORMAT>
       <TDLINE>EXECUTE_CHECK</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>AL</TDFORMAT>
       <TDLINE>Prüft Geschäftsobjekte.</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>B1</TDFORMAT>
       <TDLINE>EXECUTE_SAVE</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>AL</TDFORMAT>
       <TDLINE>Sichert Geschäftsobjekte.</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>B1</TDFORMAT>
       <TDLINE>GET_SAVE_STATUS</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>AL</TDFORMAT>
       <TDLINE>Returniert den Status der Sicherung.</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>AL</TDFORMAT>
       <TDLINE>Die möglichen Werte sind durch Konstanten CO_SAVESTAT_* im Interface</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>&lt;DS:IF.IF_ISH_BUSINESS_OBJECT&gt;IF_ISH_BUSINESS_OBJECT&lt;/&gt; definiert und</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>entsprechen den Festwerten der Domäne N1BOSAVESTATUS.</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>B1</TDFORMAT>
       <TDLINE>GET_T_BO</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>AL</TDFORMAT>
       <TDLINE>Returniert die zu verarbeitenden Geschäftsobjekte.</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>B1</TDFORMAT>
       <TDLINE>HAS_BO</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>AL</TDFORMAT>
       <TDLINE>Returniert, ob ein bestimmtes Geschäftsobjekt in der Liste der zu</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>verarbeitenden Geschäftsobjekte enthalten ist.</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>U1</TDFORMAT>
       <TDLINE>&amp;RELATIONS&amp;</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>AS</TDFORMAT>
      </TLINE>
      <TLINE>
       <TDFORMAT>U1</TDFORMAT>
       <TDLINE>&amp;EXAMPLE&amp;</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>AS</TDFORMAT>
      </TLINE>
      <TLINE>
       <TDFORMAT>U1</TDFORMAT>
       <TDLINE>&amp;HINTS&amp;</TDLINE>
      </TLINE>
