    <item>
     <DOKIL>
      <ID>RE</ID>
      <OBJECT>RN1_CORRECT_CORDER_N1COMPA</OBJECT>
      <LANGU>D</LANGU>
      <TYP>E</TYP>
      <VERSION>0003</VERSION>
      <MASTERLANG>X</MASTERLANG>
      <TXTLINES>00025</TXTLINES>
