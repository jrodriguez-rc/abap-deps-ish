      <TLINE>
       <TDFORMAT>AS</TDFORMAT>
       <TDLINE>Mit diesem Kennzeichen wird definiert, dass der Patient seine</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>Einwilligung für die Durchführung einer Maßnahme (z.B. einer Operation)</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>gegeben hat.</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>AS</TDFORMAT>
       <TDLINE>Dieses Kennzeichen dient in erster Linie für Checklisten und</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>Vollständigkeitsprüfungen. Für dokumentarische Zwecke ist dieses</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>Kennzeichen nicht geeignet.</TDLINE>
      </TLINE>
     </LINES>
