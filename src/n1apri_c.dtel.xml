      <TLINE>
       <TDFORMAT>AS</TDFORMAT>
       <TDLINE>Im Klinischen Auftrag bzw. in der Anforderung belegt das System das Feld</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>Priorität mit der höchsten Priorität vor, die Sie in den Stammdaten bei</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>den angeforderten Leistungen gepflegt haben. Sie können diesen Wert</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>jederzeit ändern.</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>/:</TDFORMAT>
       <TDLINE>INCLUDE ISH_CORD_PREREG_DEF OBJECT DOKU ID TX</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>AS</TDFORMAT>
      </TLINE>
