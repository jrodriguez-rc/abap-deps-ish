  <asx:values>
   <LONGTEXTS>
    <item>
     <DOKIL>
      <ID>HY</ID>
      <OBJECT>SIMGISH_TN1TC_OU_RESP_OU</OBJECT>
      <LANGU>D</LANGU>
      <TYP>E</TYP>
      <VERSION>0009</VERSION>
      <MASTERLANG>X</MASTERLANG>
      <TXTLINES>00016</TXTLINES>
      <DOKSTATE>A</DOKSTATE>
     </DOKIL>
     <HEAD>
      <TDOBJECT>DSYS</TDOBJECT>
      <TDNAME>SIMGISH_TN1TC_OU_RESP_OU</TDNAME>
      <TDID>HY</TDID>
      <TDSPRAS>D</TDSPRAS>
      <TDFORM>S_DOCU_SHOW</TDFORM>
      <TDSTYLE>S_DOCUS1</TDSTYLE>
      <TDVERSION>00009</TDVERSION>
      <TDLINESIZE>072</TDLINESIZE>
     </HEAD>
     <LINES>
      <TLINE>
       <TDFORMAT>UT</TDFORMAT>
       <TDLINE>&amp;USE&amp;</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>AS</TDFORMAT>
       <TDLINE>In dieser Customizing-Aktivität pflegen Sie die OE-Konstellationen</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>innerhalb von OE-Zuständigkeiten (OE = Organisationseinheiten).</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>UT</TDFORMAT>
       <TDLINE>&amp;PRECONDITIONS&amp;</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>AS</TDFORMAT>
       <TDLINE>Sie haben die Customizing-Aktivität</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>=</TDFORMAT>
       <TDLINE> &lt;DS:SIMG.ISH_TN1TC_OU_RESP&gt;Bezeichnung OE-Zuständigkeiten&lt;/&gt;</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>ausgeführt.</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>UT</TDFORMAT>
       <TDLINE>&amp;STANDARD_SETUP&amp;</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>AS</TDFORMAT>
      </TLINE>
      <TLINE>
       <TDFORMAT>UT</TDFORMAT>
       <TDLINE>&amp;ACTIVITY&amp;</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>AS</TDFORMAT>
       <TDLINE>Tragen Sie in das Feld &lt;LS&gt;FachOE&lt;/&gt; den Fachbereich ein.</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>AS</TDFORMAT>
       <TDLINE>Geben Sie im Feld &lt;LS&gt;PflegOE&lt;/&gt; die pflegerische oder behandelnde</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>OE ein.</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>AS</TDFORMAT>
       <TDLINE>Beide Felder erlauben die Eingabe eines Sterns (&lt;LB&gt;*&lt;/&gt;) als</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>Platzhalter. In der Anwendung wird der Stern für alle Kombinationen der</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>betreffenden OE aufgelöst.</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>UT</TDFORMAT>
       <TDLINE>&amp;EXAMPLE&amp;</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>AS</TDFORMAT>
      </TLINE>
     </LINES>
    </item>
    <item>
     <DOKIL>
      <ID>HY</ID>
      <OBJECT>SIMGISH_TN1TC_OU_RESP_OU</OBJECT>
      <LANGU>E</LANGU>
      <TYP>E</TYP>
      <VERSION>0009</VERSION>
      <TXTLINES>00009</TXTLINES>
      <DOKSTATE>A</DOKSTATE>
     </DOKIL>
     <HEAD>
      <TDOBJECT>DSYS</TDOBJECT>
      <TDNAME>SIMGISH_TN1TC_OU_RESP_OU</TDNAME>
      <TDID>HY</TDID>
      <TDSPRAS>E</TDSPRAS>
      <TDFORM>S_DOCU_SHOW</TDFORM>
      <TDSTYLE>S_DOCUS1</TDSTYLE>
      <TDVERSION>00009</TDVERSION>
      <TDLINESIZE>072</TDLINESIZE>
     </HEAD>
     <LINES>
      <TLINE>
       <TDFORMAT>UT</TDFORMAT>
       <TDLINE>&amp;USE&amp;</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>AS</TDFORMAT>
       <TDLINE>In this Customizing activity, you maintain the OU constellations within</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>OU jurisdictions (OU = organizational unit).</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>UT</TDFORMAT>
       <TDLINE>&amp;PRECONDITIONS&amp;</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>AS</TDFORMAT>
       <TDLINE>You have performed the Customizing activity &lt;DS:SIMG.ISH_TN1TC_OU_RESP&gt;</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>=</TDFORMAT>
       <TDLINE>Texts for OU Jurisdictions&lt;/&gt;.</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>UT</TDFORMAT>
       <TDLINE>&amp;STANDARD_SETUP&amp;</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>AS</TDFORMAT>
      </TLINE>
      <TLINE>
       <TDFORMAT>UT</TDFORMAT>
       <TDLINE>&amp;ACTIVITY&amp;</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>AS</TDFORMAT>
       <TDLINE>Enter the department in the &lt;LS&gt;DeptOU&lt;/&gt; field.</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>AS</TDFORMAT>
       <TDLINE>Enter the nursing or treatment OU in the &lt;LS&gt;Nurs. OU&lt;/&gt; field.</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>AS</TDFORMAT>
       <TDLINE>You can enter an asterisk (&lt;LB&gt;*&lt;/&gt;) as a placeholder in both these</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>fields. The application resolves the asterisk for all combinations of</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>the OU in question.</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>UT</TDFORMAT>
       <TDLINE>&amp;EXAMPLE&amp;</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>AS</TDFORMAT>
      </TLINE>
     </LINES>
    </item>
    <item>
     <DOKIL>
      <ID>HY</ID>
      <OBJECT>SIMGISH_TN1TC_OU_RESP_OU</OBJECT>
      <LANGU>S</LANGU>
      <TYP>E</TYP>
      <VERSION>0009</VERSION>
      <TXTLINES>00010</TXTLINES>
      <DOKSTATE>A</DOKSTATE>
     </DOKIL>
     <HEAD>
      <TDOBJECT>DSYS</TDOBJECT>
      <TDNAME>SIMGISH_TN1TC_OU_RESP_OU</TDNAME>
      <TDID>HY</TDID>
      <TDSPRAS>S</TDSPRAS>
      <TDFORM>S_DOCU_SHOW</TDFORM>
      <TDSTYLE>S_DOCUS1</TDSTYLE>
      <TDVERSION>00009</TDVERSION>
      <TDLINESIZE>072</TDLINESIZE>
     </HEAD>
     <LINES>
      <TLINE>
       <TDFORMAT>UT</TDFORMAT>
       <TDLINE>&amp;USE&amp;</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>AS</TDFORMAT>
       <TDLINE>En esta actividad de Customizing se actualizan las constelaciones de la</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>UO dentro de las responsabilidades de la UO (UO = unidades o</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>=</TDFORMAT>
       <TDLINE>rganizativas).</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>UT</TDFORMAT>
       <TDLINE>&amp;PRECONDITIONS&amp;</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>AS</TDFORMAT>
       <TDLINE>Debe haber ejecutado la actividad de Customizing</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>&lt;DS:SIMG.ISH_TN1TC_OU_RESP&gt;Denominación de responsabilidades UO&lt;/&gt;.</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>UT</TDFORMAT>
       <TDLINE>&amp;STANDARD_SETUP&amp;</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>AS</TDFORMAT>
      </TLINE>
      <TLINE>
       <TDFORMAT>UT</TDFORMAT>
       <TDLINE>&amp;ACTIVITY&amp;</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>AS</TDFORMAT>
       <TDLINE>Indique en el campo &lt;LS&gt;UOMéd&lt;/&gt; el departamento.</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>AS</TDFORMAT>
       <TDLINE>Indique en el campo &lt;LS&gt;UOEnf&lt;/&gt; la UO de enfermería o de tratamiento.</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>AS</TDFORMAT>
       <TDLINE>Ambos campos permiten la entrada de un asterisco (&lt;LB&gt;*&lt;/&gt;) como</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>comodín. En la aplicación utiliza el asterisco para todas las</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>combinaciones de la UO en cuestión.</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>UT</TDFORMAT>
       <TDLINE>&amp;EXAMPLE&amp;</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>AS</TDFORMAT>
      </TLINE>
     </LINES>
    </item>
   </LONGTEXTS>
