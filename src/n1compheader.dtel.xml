   <LONGTEXTS>
    <item>
     <DOKIL>
      <ID>DE</ID>
      <OBJECT>N1COMPHEADER</OBJECT>
      <LANGU>D</LANGU>
      <TYP>E</TYP>
      <VERSION>0004</VERSION>
      <MASTERLANG>X</MASTERLANG>
      <TXTLINES>00012</TXTLINES>
      <DOKSTATE>A</DOKSTATE>
     </DOKIL>
     <HEAD>
      <TDOBJECT>DOKU</TDOBJECT>
      <TDNAME>N1COMPHEADER</TDNAME>
      <TDID>DE</TDID>
      <TDSPRAS>D</TDSPRAS>
      <TDFORM>S_DOCU_SHOW</TDFORM>
      <TDSTYLE>S_DOCUS1</TDSTYLE>
      <TDVERSION>00004</TDVERSION>
      <TDLINESIZE>072</TDLINESIZE>
     </HEAD>
     <LINES>
      <TLINE>
       <TDFORMAT>U1</TDFORMAT>
       <TDLINE>&amp;DEFINITION&amp;</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>AS</TDFORMAT>
       <TDLINE>Dieses Kennzeichen gibt an, ob es sich um einen Kopfbaustein oder einen</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>Positionsbaustein des Klinischen Auftrages handelt.</TDLINE>
      </TLINE>
      <TLINE/>
      <TLINE>
       <TDFORMAT>/:</TDFORMAT>
       <TDLINE>INCLUDE ISH_CORD_PREREG_DEF OBJECT DOKU ID TX</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>B1</TDFORMAT>
       <TDLINE>&lt;lb&gt;X&lt;/&gt;: Kopfbaustein</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>B1</TDFORMAT>
       <TDLINE>&lt;LB&gt;&apos; &apos;&lt;/&gt;: Positionsbaustein</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>U1</TDFORMAT>
       <TDLINE>&amp;USE&amp;</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>AS</TDFORMAT>
       <TDLINE>Das System stellt Kopfbausteine im Klinischen Auftrag allen</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>Auftragspositionen voran. Es stellt gleichartige Kopfbausteine aus</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>unterschiedlichen Auftragstypen (Positionen) nur einmal dar.</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>AS</TDFORMAT>
       <TDLINE>Das System stellt Positionsbausteine in den Positionen auf den im</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>Auftragstyp definierten Registerkarten dar.</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>AS</TDFORMAT>
       <TDLINE>Dieser unterschiedliche Gültigkeitsbereich von Bausteinen gilt auch für</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>solche, die keine eigene Oberfläche (Dynpro) implementieren.</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>U1</TDFORMAT>
       <TDLINE>&amp;DEPENDENCIES&amp;</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>AS</TDFORMAT>
      </TLINE>
      <TLINE>
       <TDFORMAT>U1</TDFORMAT>
       <TDLINE>&amp;EXAMPLE&amp;</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>AS</TDFORMAT>
      </TLINE>
     </LINES>
    </item>
    <item>
     <DOKIL>
      <ID>DE</ID>
      <OBJECT>N1COMPHEADER</OBJECT>
      <LANGU>E</LANGU>
      <TYP>E</TYP>
      <VERSION>0004</VERSION>
      <TXTLINES>00012</TXTLINES>
      <DOKSTATE>A</DOKSTATE>
     </DOKIL>
     <HEAD>
      <TDOBJECT>DOKU</TDOBJECT>
      <TDNAME>N1COMPHEADER</TDNAME>
      <TDID>DE</TDID>
      <TDSPRAS>E</TDSPRAS>
      <TDFORM>S_DOCU_SHOW</TDFORM>
      <TDSTYLE>S_DOCUS1</TDSTYLE>
      <TDVERSION>00004</TDVERSION>
      <TDLINESIZE>072</TDLINESIZE>
     </HEAD>
     <LINES>
      <TLINE>
       <TDFORMAT>U1</TDFORMAT>
       <TDLINE>&amp;DEFINITION&amp;</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>AS</TDFORMAT>
       <TDLINE>This indicator specifies whether the component is a header component or</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>item component of the clinical order.</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>/:</TDFORMAT>
       <TDLINE>INCLUDE ISH_CORD_PREREG_DEF OBJECT DOKU ID TX</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>B1</TDFORMAT>
       <TDLINE>&lt;LB&gt;X&lt;/&gt;: Header component</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>B1</TDFORMAT>
       <TDLINE>&lt;LB&gt;&apos; &apos;&lt;/&gt;: Item component</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>U1</TDFORMAT>
       <TDLINE>&amp;USE&amp;</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>AS</TDFORMAT>
       <TDLINE>The system precedes all order items with header components in the</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>clinical order. It displays the same kind of header components from</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>different order types (items) once only.</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>AS</TDFORMAT>
       <TDLINE>The system displays item components in the items on the tab pages</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>defined in the order type.</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>AS</TDFORMAT>
       <TDLINE>This different validity period of components also applies for components</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>that do not implement their own interface (screen).</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>U1</TDFORMAT>
       <TDLINE>&amp;DEPENDENCIES&amp;</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>AS</TDFORMAT>
      </TLINE>
      <TLINE>
       <TDFORMAT>U1</TDFORMAT>
       <TDLINE>&amp;EXAMPLE&amp;</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>AS</TDFORMAT>
      </TLINE>
     </LINES>
    </item>
    <item>
     <DOKIL>
      <ID>DE</ID>
      <OBJECT>N1COMPHEADER</OBJECT>
      <LANGU>S</LANGU>
      <TYP>E</TYP>
      <VERSION>0004</VERSION>
      <TXTLINES>00014</TXTLINES>
      <DOKSTATE>A</DOKSTATE>
     </DOKIL>
     <HEAD>
      <TDOBJECT>DOKU</TDOBJECT>
      <TDNAME>N1COMPHEADER</TDNAME>
      <TDID>DE</TDID>
      <TDSPRAS>S</TDSPRAS>
      <TDFORM>S_DOCU_SHOW</TDFORM>
      <TDSTYLE>S_DOCUS1</TDSTYLE>
      <TDVERSION>00004</TDVERSION>
      <TDLINESIZE>072</TDLINESIZE>
     </HEAD>
     <LINES>
      <TLINE>
       <TDFORMAT>U1</TDFORMAT>
       <TDLINE>&amp;DEFINITION&amp;</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>AS</TDFORMAT>
       <TDLINE>Indica si se trata de un módulo de cabecera o de un módulo de posición</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>de la orden clínica.</TDLINE>
      </TLINE>
      <TLINE/>
      <TLINE>
       <TDFORMAT>/:</TDFORMAT>
       <TDLINE>INCLUDE ISH_CORD_PREREG_DEF OBJECT DOKU ID TX LANGUAGE ES</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>B1</TDFORMAT>
       <TDLINE>&lt;lb&gt;X&lt;/&gt;: módulo de cabecera</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>B1</TDFORMAT>
       <TDLINE>&lt;LB&gt;&apos; &apos;&lt;/&gt;: módulo de posición</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>U1</TDFORMAT>
       <TDLINE>&amp;USE&amp;</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>AS</TDFORMAT>
       <TDLINE>El sistema hace preceder los módulos de cabecera de la orden clínica</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>a todas las posiciones de orden y representa sólo una vez</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>los módulos de cabecera de la misma categoría de tipos de orden</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>distintos (posiciones).</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>AS</TDFORMAT>
       <TDLINE>El sistema representa los módulos de posición en las posiciones</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>definidas en el tipo de orden.</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>AS</TDFORMAT>
       <TDLINE>Estos sectores de validez del módulo, distintos,</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>también son válidos para aquellos</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>que no implementan superficies (dynpro).</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>U1</TDFORMAT>
       <TDLINE>&amp;DEPENDENCIES&amp;</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>AS</TDFORMAT>
      </TLINE>
      <TLINE>
       <TDFORMAT>U1</TDFORMAT>
       <TDLINE>&amp;EXAMPLE&amp;</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>AS</TDFORMAT>
      </TLINE>
     </LINES>
    </item>
   </LONGTEXTS>
