  <asx:values>
   <VSEOINTERF>
    <CLSNAME>IF_ISH_CB_DESTROYABLE</CLSNAME>
    <LANGU>E</LANGU>
    <DESCRIPT>IS-H: Callback for a Destroyable Object</DESCRIPT>
    <EXPOSURE>2</EXPOSURE>
    <STATE>1</STATE>
    <UNICODE>X</UNICODE>
   </VSEOINTERF>
   <LINES>
    <TLINE>
     <TDFORMAT>U1</TDFORMAT>
     <TDLINE>&amp;DESCRIPTION&amp;</TDLINE>
    </TLINE>
    <TLINE>
     <TDFORMAT>AS</TDFORMAT>
     <TDLINE>Interface IF_ISH_CB_DESTROYABLE is used to prohibit or allow the</TDLINE>
    </TLINE>
    <TLINE>
     <TDLINE>destruction of a &lt;DS:IF.IF_ISH_DESTROYABLE&gt;destroyable object&lt;/&gt;.</TDLINE>
    </TLINE>
    <TLINE>
     <TDFORMAT>UT</TDFORMAT>
     <TDLINE>Methods:</TDLINE>
    </TLINE>
    <TLINE>
     <TDFORMAT>B1</TDFORMAT>
     <TDLINE>CB_DESTROY</TDLINE>
    </TLINE>
    <TLINE>
     <TDFORMAT>AL</TDFORMAT>
     <TDLINE>The destroyable object calls this method before being destroyed.</TDLINE>
    </TLINE>
    <TLINE>
     <TDFORMAT>AL</TDFORMAT>
     <TDLINE>If parameter R_CONTINUE is set to &apos;X&apos;, the destroyable object continues</TDLINE>
    </TLINE>
    <TLINE>
     <TDLINE>processing. Otherwise, destruction is cancelled.</TDLINE>
    </TLINE>
    <TLINE>
     <TDFORMAT>U1</TDFORMAT>
     <TDLINE>&amp;USE&amp;</TDLINE>
    </TLINE>
    <TLINE>
     <TDFORMAT>AS</TDFORMAT>
    </TLINE>
    <TLINE>
     <TDFORMAT>U1</TDFORMAT>
     <TDLINE>&amp;EXAMPLE&amp;</TDLINE>
    </TLINE>
    <TLINE>
     <TDFORMAT>AS</TDFORMAT>
    </TLINE>
    <TLINE>
     <TDFORMAT>U1</TDFORMAT>
     <TDLINE>&amp;HINTS&amp;</TDLINE>
    </TLINE>
    <TLINE>
     <TDFORMAT>AS</TDFORMAT>
    </TLINE>
    <TLINE>
     <TDFORMAT>U1</TDFORMAT>
     <TDLINE>&amp;FURTHER_SOURCES_OF_INF&amp;</TDLINE>
    </TLINE>
    <TLINE>
     <TDFORMAT>AS</TDFORMAT>
    </TLINE>
   </LINES>
   <I18N_LINES>
    <item>
     <LANGUAGE>D</LANGUAGE>
     <LINES>
      <TLINE>
       <TDFORMAT>U1</TDFORMAT>
       <TDLINE>&amp;MEANING&amp;</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>AS</TDFORMAT>
       <TDLINE>Das Interface IF_ISH_CB_DESTROYABLE dient zum Verbieten oder Erlauben</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>der Zerstörung eines &lt;DS:IF.IF_ISH_DESTROYABLE&gt;zerstörbaren Objekts&lt;/&gt;.</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>UT</TDFORMAT>
       <TDLINE>Methoden:</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>B1</TDFORMAT>
       <TDLINE>CB_DESTROY</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>AL</TDFORMAT>
       <TDLINE>Diese Methode wird vom zerstörbaren Objekt vor dessen Zerstörung</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>aufgerufen.</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>AL</TDFORMAT>
       <TDLINE>Wird der Parameter R_CONTINUE=&apos;X&apos; gesetzt, setzt das zerstörbare Objekt</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>die Verarbeitung fort, ansonsten wird die Zerstörung abgebrochen.</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>U1</TDFORMAT>
       <TDLINE>&amp;USE&amp;</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>AS</TDFORMAT>
      </TLINE>
      <TLINE>
       <TDFORMAT>U1</TDFORMAT>
       <TDLINE>&amp;EXAMPLE&amp;</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>AS</TDFORMAT>
      </TLINE>
      <TLINE>
       <TDFORMAT>U1</TDFORMAT>
       <TDLINE>&amp;HINTS&amp;</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>AS</TDFORMAT>
      </TLINE>
      <TLINE>
       <TDFORMAT>U1</TDFORMAT>
       <TDLINE>&amp;FURTHER_SOURCES_OF_INF&amp;</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>AS</TDFORMAT>
      </TLINE>
     </LINES>
    </item>
   </I18N_LINES>
